*.color15:    #E6E6E6

! URxvt
URxvt.perl-ext-common:    default,matcher,selection-to-clipboard,keyboard-select,fullscreen
URxvt.clipboard.autocopy: true ! copy to clipboard
URxvt.keysym.M-Escape:    perl:keyboard-select:activate
URxvt.keysym.F11:         perl:fullscreen:switch
URxvt.keysym.C-y:         perl:clipboard:paste
URxvt.keysym.Button-2:    perl:clipboard:paste

URxvt.font:                      xft:MesloLGS\ Nerd\ Font\ Mono:size=10
URxvt.geometry:                  80x20
URxvt.depth:                     32
URxvt*scrollBar:                 false
URxvt*mouseWheelScrollPage:      false
URxvt*cursorBlink:               true
URxvt*saveLines:                 5000


! Rofi
rofi.separator-style: solid
rofi.sidebar-mode: false
rofi.lines: 5
rofi.font: monofur 13
rofi.bw: 1
rofi.columns: 2
rofi.padding: 5
rofi.fixed-num-lines: true
rofi.hide-scrollbar: true
