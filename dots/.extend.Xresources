*fading:                 15
*fadeColor:              black

URxvt.internalBorder: 6

Xcursor.theme: Breeze
Xcursor.size:  0

XTerm*background:        #6aa7a5
XTerm*foreground:        #6aa7a5
XTerm*reverseVideo:      on
XTerm*faceName:          HackRegular:size=11:antialias=true
XTerm*selectToClipboard: true

*.foreground: #F8F8F2
*.background: #282A36
*.color0:     #000000
*.color8:     #4D4D4D
*.color1:     #FF5555
*.color9:     #FF6E67
*.color2:     #50FA7B
*.color10:    #5AF78E
